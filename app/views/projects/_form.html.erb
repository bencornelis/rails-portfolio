<%= form_for [@skill, @project] do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control", rows: "10" %>
  </div>
  <div class="form-group">
    <%= f.label :github_url %>
    <%= f.text_field :github_url, class: "form-control", rows: "10" %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
